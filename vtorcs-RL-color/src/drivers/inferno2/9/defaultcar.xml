<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE params SYSTEM "../../libs/tgf/params.dtd">

<params name="car" type="params">
  <section name="Brake System">
    <attnum name="front-rear brake repartition" val="0.75"/>
    <attnum name="max pressure" min="100" max="1.5e+06" unit="kPa" val="30000"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" min="0" max="10000" unit="lbs/in" val="0"/>
    <attnum name="suspension course" min="0" max="1" unit="m" val="0.2"/>
    <attnum name="bellcrank" min="0.5" max="5" val="2.5"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" min="0" max="10000" unit="lbs/in" val="0"/>
    <attnum name="suspension course" min="0" max="1" unit="m" val="0.2"/>
    <attnum name="bellcrank" min="0.5" max="5" val="2.5"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" min="0" max="10000" unit="lbs/in" val="4000"/>
    <attnum name="suspension course" min="0" max="1" unit="m" val="0.25"/>
    <attnum name="packers" min="0" max="100" unit="mm" val="50"/>
    <attnum name="bellcrank" val="1"/>
    <attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="100"/>
    <attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="800"/>
    <attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="100"/>
    <attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="800"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" min="0" max="10000" unit="lbs/in" val="4000"/>
    <attnum name="suspension course" min="0" max="1" unit="m" val="0.25"/>
    <attnum name="packers" min="0" max="100" unit="mm" val="50"/>
    <attnum name="bellcrank" val="1"/>
    <attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="100"/>
    <attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="800"/>
    <attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="100"/>
    <attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="800"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" min="0" max="10000" unit="lbs/in" val="3000"/>
    <attnum name="suspension course" min="0" max="1" unit="m" val="0.25"/>
    <attnum name="packers" min="0" max="100" unit="mm" val="50"/>
    <attnum name="bellcrank" val="1"/>
    <attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="30"/>
    <attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="1000"/>
    <attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="30"/>
    <attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="1000"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" min="0" max="10000" unit="lbs/in" val="3000"/>
    <attnum name="suspension course" min="0" max="1" unit="m" val="0.25"/>
    <attnum name="packers" min="0" max="100" unit="mm" val="50"/>
    <attnum name="bellcrank" val="1"/>
    <attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="30"/>
    <attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="1000"/>
    <attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="30"/>
    <attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="1000"/>
